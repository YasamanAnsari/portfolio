  "scripts": {
    "dev": "yarn clone:tailwind && NODE_ENV=development next dev",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "postbuild": "next-sitemap",
    "lint": "next lint",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "clone:tailwind": "chmod u+x ./tailwind.generate.config.js && ./tailwind.generate.config.js && next lint --fix -d src/lib/css"
  },
  "sideEffects": [
    "./public/*",
    "./src/pages/*"
  ],
  "dependencies": {
    "next": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^1.0.0",
    "ua-parser-js": "^0.7.28"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@next/eslint-plugin-next": "^11.1.0",
    "@styled-jsx/plugin-sass": "^4.0.1",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/tailwindcss": "^2.2.1",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "autoprefixer": "^10.3.3",
    "eslint": "^7.26.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "fetch": "^1.1.0",
    "lost": "^8.3.0",
    "msw": "^0.35.0",
    "next-sitemap": "^1.6.173",
    "postcss": "^8.3.6",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.3.0",
    "sass": "^1.38.1",
    "styled-jsx-plugin-postcss": "^4.0.1",
    "stylelint": "^13.13.1",
    "tailwindcss": "^2.2.8",
    "typescript": "~4.3"
  }
}
